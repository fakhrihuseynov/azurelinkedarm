{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "VNetSettings": {
      "value": {
        "name": "VNet",
        "addressPrefixes": [
          {
            "name": "VnetPrefix",
            "addressPrefix": "10.0.0.0/22"
          }
        ],
        "subnets": [
          {
            "name": "front-end-subnet",
            "addressPrefix": "10.0.0.0/24"
          },
          {
            "name": "back-end-subnet",
            "addressPrefix": "10.0.1.0/24"
          }
        ]
      }
    },
    "StorageAccountType": {
      "value": "Standard_LRS"
    },
    "VmSettings": {
      "value": {
        "securityRules": {
          "name1": "nsgRule1",
          "properties1": {
            "description": "RDP",
            "protocol": "Tcp",
            "sourcePortRange": "*",
            "destinationPortRange": "3389",
            "sourceAddressPrefix": "*",
            "destinationAddressPrefix": "*",
            "access": "Allow",
            "priority": 100,
            "direction": "Inbound"
          },
          "name2": "nsgRule2",
          "properties2": {
            "description": "HTTP",
            "protocol": "Tcp",
            "sourcePortRange": "*",
            "destinationPortRange": "8080",
            "sourceAddressPrefix": "*",
            "destinationAddressPrefix": "*",
            "access": "Allow",
            "priority": 101,
            "direction": "Inbound"
          }
        }
      }
    }
  }
}